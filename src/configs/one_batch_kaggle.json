{
  "name": "one_batch_test",
  "n_gpu": 1,
  "preprocessing": {
    "sr": 16000,
    "spectrogram": {
      "type": "Spectrogram",
      "args": {
        "n_fft": 1724,
        "win_length": 1724
      }
    },
    "log_spec": false
  },
  "augmentations": {
    "wave": [],
    "spectrogram": []
  },
  "arch": {
    "type": "LCNN",
    "args": {}
  },
  "data": {
    "train": {
      "batch_size": 2,
      "num_workers": 0,
      "datasets": [
        {
          "type": "ASVSpoof2019Dataset",
          "args": {
            "data_dir": "/kaggle/input/asvpoof-2019-dataset",
            "index_dir": "/kaggle/working",
            "part": "train",
            "limit": 2
          }
        }
      ]
    },
    "val": {
      "batch_size": 2,
      "num_workers": 0,
      "datasets": [
        {
          "type": "ASVSpoof2019Dataset",
          "args": {
            "data_dir": "/kaggle/input/asvpoof-2019-dataset",
            "index_dir": "/kaggle/working",
            "part": "dev",
            "limit": 2
          }
        }
      ]
    },
    "test": {
      "batch_size": 2,
      "num_workers": 0,
      "datasets": [
        {
          "type": "ASVSpoof2019Dataset",
          "args": {
            "data_dir": "/kaggle/input/asvpoof-2019-dataset",
            "index_dir": "/kaggle/working",
            "part": "eval",
            "limit": 2
          }
        }
      ]
    }
  },
  "optimizer": {
    "type": "AdamW",
    "args": {
      "lr": 3e-3
    }
  },
  "loss": {
    "type": "FinalLoss",
    "args": {}
  },
  "metrics": [
    {
      "type": "EERMetric",
      "args": {
        "name": "EERMetric"
      }
    }
  ],
  "lr_scheduler": {
    "type": "OneCycleLR",
    "args": {
      "steps_per_epoch": 100,
      "epochs": 15,
      "anneal_strategy": "cos",
      "max_lr": 3e-3,
      "pct_start": 0.2
    }
  },
  "trainer": {
    "epochs": 15,
    "save_dir": "saved/",
    "save_period": 5,
    "verbosity": 2,
    "monitor": "min val_loss",
    "early_stop": 100,
    "visualize": "wandb",
    "wandb_project": "as_project",
    "len_epoch": 100,
    "grad_norm_clip": 10
  }
}
