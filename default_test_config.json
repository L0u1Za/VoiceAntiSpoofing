{
  "name": "default_config",
  "n_gpu": 1,
  "preprocessing": {
    "sr": 16000,
    "spectrogram": {
      "type": "Spectrogram",
      "args": {
        "n_fft": 1724,
        "win_length": 1724
      }
    },
    "log_spec": false
  },
  "augmentations": {
    "wave": [],
    "spectrogram": []
  },
  "arch": {
    "type": "LCNN",
    "args": {}
  },
  "data": {
    "test": {
      "batch_sampler": {
        "type": "BatchSampler",
        "args": {
          "batch_size": 16
        }
      },
      "num_workers": 5,
      "datasets": [
        {
          "type": "ASVSpoof2019Dataset",
          "args": {
            "part": "eval"
          }
        }
      ]
    }
  },
  "optimizer": {
    "type": "Adam",
    "args": {
      "lr": 3e-4,
      "betas": [0.9, 0.999]
    }
  },
  "loss": {
    "type": "FinalLoss",
    "args": {}
  },
  "metrics": [
    {
      "type": "EERMetric",
      "args": {
        "name": "EERMetric"
      }
    }
  ],
  "lr_scheduler": {
    "type": "OneCycleLR",
    "args": {
      "steps_per_epoch": 5000,
      "epochs": 10,
      "anneal_strategy": "cos",
      "max_lr": 3e-4,
      "pct_start": 0.2
    }
  },
  "trainer": {
    "epochs": 10,
    "save_dir": "saved/",
    "save_period": 5,
    "verbosity": 2,
    "monitor": "min val_loss",
    "early_stop": 100,
    "visualize": "wandb",
    "wandb_project": "as_project",
    "len_epoch": 5000,
    "grad_norm_clip": 10
  }
}
